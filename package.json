{
  "name": "redex",
  "description": "redex - Reason package index",
  "version": "1.0.0",
  "author": "glennsl",
  "dependencies": {
    "@glennsl/bs-json": "^1.1.2",
    "@glennsl/rebase": "^0.2.0",
    "algoliasearch": "^3.24.9",
    "algoliasearch-helper": "^2.23.2",
    "bs-css": "^5.4.0",
    "gatsby": "^1.9.158",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-glamor": "^1.6.10",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-source-filesystem": "^1.5.11",
    "gatsby-transformer-json": "^1.0.14",
    "glamor-reset": "^3.0.0-3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-timeago": "^3.4.3",
    "remark": "^8.0.0",
    "remark-html": "^7.0.0",
    "vrroom": "github:glennsl/vrroom"
  },
  "keywords": [
    "reason",
    "reasonml",
    "package index",
    "repository"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "bsb": "bsb -make-world -w",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "test": "validate_sources",
    "deploy": "gatsby build && update_search_index && gh-pages -d public -b master",
    "clean": "rm -rf data/generated",
    "update-sources": "git submodule update --remote && validate_sources",
    "fetch": "npm run clean && npm run update-sources && fetch_published && fetch_unpublished",
    "everything": "npm run fetch && npm run deploy"
  },
  "devDependencies": {
    "bs-platform": "^2.0.0",
    "gh-pages": "^1.1.0",
    "github-slugger": "^1.2.0",
    "mdast-util-to-string": "^1.0.4",
    "reason": "^3.0.2",
    "reason-react": "^0.3.1",
    "redex-scripts": "redex/redex-scripts"
  }
}
